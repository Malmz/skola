package hello_world;

public class Encrypt {

	char c1;
	String s1;
	String s2;

	public int getNum(char c1) {
		
		int num = (int)c1;
		if(num <= 122 & num >= 97){
			return num-96;
		}
		return -1;
	}
	
	public String decrypt(String s1) {
		for (int i = 0; i < s1.length();) {
			int jump = getNum(s1.charAt(i));
			s2 += s1.charAt(i);
			i += jump;
		}
		
		return "";
	}
	
	public static void main(String[] args) {
		System.out.println(getNum('a'));
		
		
	}
	
}
